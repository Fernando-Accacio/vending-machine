{
  "version": 2,
  
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/angular"
    }
  ],
  
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "dest": "/index.html"
    }
  ]
}