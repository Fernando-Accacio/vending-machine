{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,

  "installCommand": "cd frontend && npm install",
  
  "buildCommand": "cd frontend && npm run build",
  
  "outputDirectory": "frontend/dist/dish-app",

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}